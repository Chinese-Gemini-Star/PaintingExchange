                "auth": {
                    "description": "图片作者用户名",
                    "type": "string",
                    "example": "test"
                },
                "authIsBan": {
                    "description": "作者是否被封禁",
                    "type": "boolean",
                    "example": false
                },
                "bigURI": {
                    "description": "大图地址",
